func int main() {
    str s;
    str t;

    s = "Hello!";
    t = "Goodbye!";
    
	printStr(s);
	print(strLen(s));
	printStr(t);
	print(strLen(t));

    freeStr(s);
    s = t;

	if(strEq(s,t)) {
		printStr("equal");
	} else {
		printStr("not equal");
	}

    t = ":)";

	if(strEq(s,t)) {
		printStr("equal");
	} else {
		printStr("not equal");
	}
	
	printStr(s);
	printStr(t);

    freeStr(t);

    print(1);

    return 0;
}

