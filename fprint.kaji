func int main() {
    str s;
    s = "This should be at beginning of file.\n";

    printStr("Writing to tmp.txt...");
    fprintStr("tmp.txt", s, 0);
    printStr("Done.\n");

    printStr("Appending to tmp.txt...");
    fprintStr("tmp.txt", "This should be at end of file.\n", 1);
    printStr("Done.");

    return 0;
}